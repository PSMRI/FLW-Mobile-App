<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="org.piramalswasthya.sakhi.ui.home_activity.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_nav_home_to_allBenFragment"
            app:destination="@id/allBenFragment" />
        <action
            android:id="@+id/action_nav_home_to_allHouseholdFragment"
            app:destination="@id/allHouseholdFragment" />
        <action
            android:id="@+id/action_nav_home_to_childCareFragment"
            app:destination="@id/childCareFragment" />
        <action
            android:id="@+id/action_nav_home_to_motherCareFragment"
            app:destination="@id/motherCareFragment" />
        <action
            android:id="@+id/action_nav_home_to_deathReportsFragment"
            app:destination="@id/deathReportsFragment" />
        <action
            android:id="@+id/action_nav_home_to_generalOpCareFragment"
            app:destination="@id/generalOpCareFragment" />
        <action
            android:id="@+id/action_nav_home_to_hrpCasesFragment"
            app:destination="@id/hrpCasesFragment" />
        <action
            android:id="@+id/action_nav_home_to_immunizationDueFragment"
            app:destination="@id/immunizationDueFragment" />
        <action
            android:id="@+id/action_nav_home_to_menopauseStageFragment"
            app:destination="@id/menopauseStageFragment" />
        <action
            android:id="@+id/action_nav_home_to_ncdFragment"
            app:destination="@id/ncdFragment" />
        <action
            android:id="@+id/action_nav_home_to_villageLevelFormsFragment"
            app:destination="@id/villageLevelFormsFragment" />
        <action
            android:id="@+id/action_nav_home_to_eligibleCoupleFragment"
            app:destination="@id/eligibleCoupleFragment" />
        <action
            android:id="@+id/action_nav_home_to_serviceTypeFragment"
            app:destination="@id/serviceTypeFragment" />
        <action
            android:id="@+id/action_homeFragment_to_newHouseholdFragment"
            app:destination="@id/newHouseholdFragment" />
    </fragment>
    <fragment
        android:id="@+id/allHouseholdFragment"
        android:name="org.piramalswasthya.sakhi.ui.home_activity.all_household.AllHouseholdFragment"
        android:label="@string/home_icon_all_household"
        tools:layout="@layout/fragment_display_search_rv_button" >
        <action
            android:id="@+id/action_allHouseholdFragment_to_newHouseholdFragment"
            app:destination="@id/newHouseholdFragment" />
        <action
            android:id="@+id/action_allHouseholdFragment_to_newBenRegTypeFragment"
            app:destination="@id/newBenRegTypeFragment" />
    </fragment>
    <fragment
        android:id="@+id/allBenFragment"
        android:name="org.piramalswasthya.sakhi.ui.home_activity.all_ben.AllBenFragment"
        android:label="@string/home_icon_title_all_ben"
        tools:layout="@layout/fragment_all_ben" >
        <action
            android:id="@+id/action_allBenFragment_to_newBenRegTypeFragment"
            app:destination="@id/newBenRegTypeFragment" />
    </fragment>
    <fragment
        android:id="@+id/eligibleCoupleFragment"
        android:name="org.piramalswasthya.sakhi.ui.home_activity.eligible_couple.EligibleCoupleFragment"
        android:label="@string/home_icon_title_elig_coup"
        tools:layout="@layout/fragment_eligible_couple" />
    <fragment
        android:id="@+id/childCareFragment"
        android:name="org.piramalswasthya.sakhi.ui.home_activity.child_care.ChildCareFragment"
        android:label="@string/home_icon_title_child_care"
        tools:layout="@layout/rv_icon_grid" >
        <action
            android:id="@+id/action_childCareFragment_to_infantListFragment"
            app:destination="@id/infantListFragment" />
        <action
            android:id="@+id/action_childCareFragment_to_adolescentListFragment"
            app:destination="@id/adolescentListFragment" />
        <action
            android:id="@+id/action_childCareFragment_to_childListFragment"
            app:destination="@id/childListFragment" />
    </fragment>
    <fragment
        android:id="@+id/motherCareFragment"
        android:name="org.piramalswasthya.sakhi.ui.home_activity.mother_care.MotherCareFragment"
        android:label="@string/home_icon_title_mother_care"
        tools:layout="@layout/rv_icon_grid" >
        <action
            android:id="@+id/action_motherCareFragment_to_pregnancyListFragment"
            app:destination="@id/pregnancyListFragment" />
        <action
            android:id="@+id/action_motherCareFragment_to_deliveryStageListFragment"
            app:destination="@id/deliveryStageListFragment" />
        <action
            android:id="@+id/action_motherCareFragment_to_pncMotherListFragment"
            app:destination="@id/pncMotherListFragment" />
        <action
            android:id="@+id/action_motherCareFragment_to_reproductiveAgeListFragment"
            app:destination="@id/reproductiveAgeListFragment" />
    </fragment>
    <fragment
        android:id="@+id/deathReportsFragment"
        android:name="org.piramalswasthya.sakhi.ui.home_activity.death_reports.DeathReportsFragment"
        android:label="@string/home_icon_title_death_rep"
        tools:layout="@layout/fragment_death_reports" />
    <fragment
        android:id="@+id/generalOpCareFragment"
        android:name="org.piramalswasthya.sakhi.ui.home_activity.general_op_care.GeneralOpCareFragment"
        android:label="@string/home_icon_title_gen_op"
        tools:layout="@layout/fragment_general_op_care" />
    <fragment
        android:id="@+id/hrpCasesFragment"
        android:name="org.piramalswasthya.sakhi.ui.home_activity.hrp_cases.HrpCasesFragment"
        android:label="@string/home_icon_title_hrp_cases"
        tools:layout="@layout/fragment_hrp_cases" />
    <fragment
        android:id="@+id/immunizationDueFragment"
        android:name="org.piramalswasthya.sakhi.ui.home_activity.immunization_due.ImmunizationDueFragment"
        android:label="@string/home_icon_title_imm_due"
        tools:layout="@layout/fragment_immunization_due" />
    <fragment
        android:id="@+id/menopauseStageFragment"
        android:name="org.piramalswasthya.sakhi.ui.home_activity.menopause_stage.MenopauseStageFragment"
        android:label="@string/home_icon_title_menopause_stage_list"
        tools:layout="@layout/fragment_menopause_stage" />
    <fragment
        android:id="@+id/ncdFragment"
        android:name="org.piramalswasthya.sakhi.ui.home_activity.non_communicable_disease.NcdFragment"
        android:label="@string/home_icon_title_ncd"
        tools:layout="@layout/rv_icon_grid" >
        <action
            android:id="@+id/action_ncdFragment_to_ncdEligibleListFragment"
            app:destination="@id/ncdEligibleListFragment" />
        <action
            android:id="@+id/action_ncdFragment_to_ncdListFragment"
            app:destination="@id/ncdListFragment" />
        <action
            android:id="@+id/action_ncdFragment_to_ncdNonEligibleListFragment"
            app:destination="@id/ncdNonEligibleListFragment" />
        <action
            android:id="@+id/action_ncdFragment_to_ncdPriorityListFragment"
            app:destination="@id/ncdPriorityListFragment" />
    </fragment>
    <fragment
        android:id="@+id/villageLevelFormsFragment"
        android:name="org.piramalswasthya.sakhi.ui.home_activity.village_level_forms.VillageLevelFormsFragment"
        android:label="@string/home_icon_title_village_lvl_forms"
        tools:layout="@layout/rv_icon_grid" >
        <action
            android:id="@+id/action_villageLevelFormsFragment_to_surveyRegisterFragment"
            app:destination="@id/surveyRegisterFragment" />
    </fragment>
    <fragment
        android:id="@+id/serviceTypeFragment"
        android:name="org.piramalswasthya.sakhi.ui.home_activity.service_type.ServiceTypeFragment"
        android:label="@string/frag_service_type_title"
        tools:layout="@layout/fragment_service_type" >
        <action
            android:id="@+id/action_serviceTypeFragment_to_nav_home"
            app:destination="@id/homeFragment"
            app:popUpTo="@id/homeFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/newHouseholdFragment"
        android:name="org.piramalswasthya.sakhi.ui.home_activity.all_household.new_household_registration.NewHouseholdFragment"
        android:label="@string/frag_nhhr_title"
        tools:layout="@layout/fragment_new_household" >
        <action
            android:id="@+id/action_newHouseholdFragment_to_newBenRegTypeFragment"
            app:destination="@id/newBenRegTypeFragment"
            app:popUpTo="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/newBenRegTypeFragment"
        android:name="org.piramalswasthya.sakhi.ui.home_activity.all_ben.new_ben_registration.NewBenRegTypeFragment"
        android:label="@string/frag_new_ben_reg_type_title"
        tools:layout="@layout/fragment_new_ben_reg_type" >
        <argument
            android:name="hhId"
            app:argType="long" />
        <action
            android:id="@+id/action_newBenRegTypeFragment_to_newBenRegL15Fragment"
            app:destination="@id/newBenRegL15Fragment" />
        <action
            android:id="@+id/action_newBenRegTypeFragment_to_newBenRegG15Fragment"
            app:destination="@id/newBenRegG15Fragment" />
    </fragment>
    <fragment
        android:id="@+id/newBenRegL15Fragment"
        android:name="org.piramalswasthya.sakhi.ui.home_activity.all_ben.new_ben_registration.ben_age_less_15.NewBenRegL15Fragment"
        android:label="Kid Registration" >
        <argument
            android:name="hhId"
            app:argType="long" />
    </fragment>
    <fragment
        android:id="@+id/newBenRegG15Fragment"
        android:name="org.piramalswasthya.sakhi.ui.home_activity.all_ben.new_ben_registration.ben_age_more_15.NewBenRegG15Fragment"
        android:label="Adult Registration" >
        <argument
            android:name="hhId"
            app:argType="long" />
    </fragment>
    <fragment
        android:id="@+id/infantListFragment"
        android:name="org.piramalswasthya.sakhi.ui.home_activity.child_care.infant_list.InfantListFragment"
        android:label="@string/child_care_icon_title_infant_list"
        tools:layout="@layout/fragment_infant_list" />
    <fragment
        android:id="@+id/adolescentListFragment"
        android:name="org.piramalswasthya.sakhi.ui.home_activity.child_care.adolescent_list.AdolescentListFragment"
        android:label="@string/child_care_icon_title_adolescent_list"
        tools:layout="@layout/fragment_adolescent_list" />
    <fragment
        android:id="@+id/childListFragment"
        android:name="org.piramalswasthya.sakhi.ui.home_activity.child_care.child_list.ChildListFragment"
        android:label="@string/child_care_icon_title_child_list"
        tools:layout="@layout/fragment_child_list" />
    <fragment
        android:id="@+id/pregnancyListFragment"
        android:name="org.piramalswasthya.sakhi.ui.home_activity.mother_care.pregnancy_list.PregnancyListFragment"
        android:label="fragment_pregnancy_list"
        tools:layout="@layout/fragment_pregnancy_list" />
    <fragment
        android:id="@+id/deliveryStageListFragment"
        android:name="org.piramalswasthya.sakhi.ui.home_activity.mother_care.delivery_stage_list.DeliveryStageListFragment"
        android:label="fragment_delivery_stage_list"
        tools:layout="@layout/fragment_delivery_stage_list" />
    <fragment
        android:id="@+id/pncMotherListFragment"
        android:name="org.piramalswasthya.sakhi.ui.home_activity.mother_care.pnc_mother_list.PncMotherListFragment"
        android:label="fragment_pnc_mother_list"
        tools:layout="@layout/fragment_pnc_mother_list" />
    <fragment
        android:id="@+id/reproductiveAgeListFragment"
        android:name="org.piramalswasthya.sakhi.ui.home_activity.mother_care.reproductive_age_list.ReproductiveAgeListFragment"
        android:label="fragment_reproductive_age_list"
        tools:layout="@layout/fragment_reproductive_age_list" />
    <fragment
        android:id="@+id/ncdEligibleListFragment"
        android:name="org.piramalswasthya.sakhi.ui.home_activity.non_communicable_disease.ncd_eligible_list.NcdEligibleListFragment"
        android:label="fragment_ncd_eligible_list"
        tools:layout="@layout/fragment_ncd_eligible_list" />
    <fragment
        android:id="@+id/ncdListFragment"
        android:name="org.piramalswasthya.sakhi.ui.home_activity.non_communicable_disease.ncd_list.NcdListFragment"
        android:label="fragment_ncd_list"
        tools:layout="@layout/fragment_ncd_list" />
    <fragment
        android:id="@+id/ncdNonEligibleListFragment"
        android:name="org.piramalswasthya.sakhi.ui.home_activity.non_communicable_disease.ncd_non_eligible_list.NcdNonEligibleListFragment"
        android:label="fragment_ncd_non_eligible_list"
        tools:layout="@layout/fragment_ncd_non_eligible_list" />
    <fragment
        android:id="@+id/ncdPriorityListFragment"
        android:name="org.piramalswasthya.sakhi.ui.home_activity.non_communicable_disease.ncd_priority_list.NcdPriorityListFragment"
        android:label="fragment_ncd_priority_list"
        tools:layout="@layout/fragment_ncd_priority_list" />
    <fragment
        android:id="@+id/surveyRegisterFragment"
        android:name="org.piramalswasthya.sakhi.ui.home_activity.village_level_forms.survey_register.SurveyRegisterFragment"
        android:label="fragment_survey_register"
        tools:layout="@layout/fragment_survey_register" />

</navigation>